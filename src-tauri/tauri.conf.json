{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "app": {
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconAsTemplate": true,
      "iconPath": "icons/icon.png"
    },
    "windows": [
      {
        "decorations": false,
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "wollama chat",
        "width": 800,
        "visible": true,
        "label": "main",
        "transparent": false
      }
    ],
    "withGlobalTauri": true
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5214",
    "frontendDist": "../build"
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "identifier": "com.medyll.wollama",
  "plugins": {
    "updater": {}
  },
  "productName": "wollama",
  "version": "0.1.0"
}